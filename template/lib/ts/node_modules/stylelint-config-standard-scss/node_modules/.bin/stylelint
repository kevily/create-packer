#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/开发/github_repo/create-packer/node_modules/.pnpm/stylelint@16.2.1_typescript@5.3.3/node_modules/stylelint/bin/node_modules:/mnt/d/开发/github_repo/create-packer/node_modules/.pnpm/stylelint@16.2.1_typescript@5.3.3/node_modules/stylelint/node_modules:/mnt/d/开发/github_repo/create-packer/node_modules/.pnpm/stylelint@16.2.1_typescript@5.3.3/node_modules:/mnt/d/开发/github_repo/create-packer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/开发/github_repo/create-packer/node_modules/.pnpm/stylelint@16.2.1_typescript@5.3.3/node_modules/stylelint/bin/node_modules:/mnt/d/开发/github_repo/create-packer/node_modules/.pnpm/stylelint@16.2.1_typescript@5.3.3/node_modules/stylelint/node_modules:/mnt/d/开发/github_repo/create-packer/node_modules/.pnpm/stylelint@16.2.1_typescript@5.3.3/node_modules:/mnt/d/开发/github_repo/create-packer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../stylelint@16.2.1_typescript@5.3.3/node_modules/stylelint/bin/stylelint.mjs" "$@"
else
  exec node  "$basedir/../../../../../stylelint@16.2.1_typescript@5.3.3/node_modules/stylelint/bin/stylelint.mjs" "$@"
fi
