{
    "name": "react-vite",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "prepare": "husky install",
        "dev": "vite",
        "build": "tsc --noEmit && vite build",
        "build:analyse": "tsc --noEmit && vite build --mode analyse",
        "preview": "vite preview",
        "up:vite": "pnpm up vite @vitejs/* vite-plugin-* -L",
        "format": "prettier --write \"**/*.{ts,js,tsx,jsx,json,css,scss,less}\"",
        "lint": "tsc --noEmit && eslint --no-error-on-unmatched-pattern && stylelint **/*.{css,scss,less,ts,tsx}",
        "lint:fix": "eslint --fix && stylelint **/*.{css,scss,less,ts,tsx} --fix",
        "cz": "cz",
        "push": "npm run commit && git push",
        "commit": "git add . && npm run cz"
    },
    "dependencies": {
        "@tanstack/react-query": "5.51.15",
        "@vanilla-extract/css": "1.17.4",
        "axios": "1.7.9",
        "define-zustand": "3.4.0",
        "es-toolkit": "1.39.8",
        "immer": "10.0.1",
        "qs": "6.11.2",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-router": "7.5.0",
        "react-use": "17.5.0",
        "type-fest": "4.33.0",
        "zustand": "5.0.3"
    },
    "devDependencies": {
        "@commitlint/cli": "17.6.1",
        "@commitlint/config-conventional": "17.6.1",
        "@commitlint/cz-commitlint": "17.5.0",
        "@emotion/babel-plugin": "11.13.5",
        "@eslint/js": "9.15.0",
        "@faker-js/faker": "8.4.1",
        "@types/node": "18.16.0",
        "@types/qs": "6.9.7",
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
        "@vanilla-extract/vite-plugin": "5.1.1",
        "@vitejs/plugin-react": "4.3.4",
        "autoprefixer": "10.4.14",
        "commitizen": "4.3.0",
        "cssnano": "6.0.0",
        "eslint": "9.17.0",
        "eslint-import-resolver-typescript": "3.7.0",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-react": "7.37.2",
        "eslint-plugin-react-hooks": "5.1.0",
        "globals": "15.12.0",
        "husky": "9.1.6",
        "inquirer": "^8.1.2",
        "postcss": "8.4.35",
        "postcss-import": "16.0.1",
        "postcss-nesting": "12.0.3",
        "prettier": "3.2.5",
        "rollup-plugin-visualizer": "5.12.0",
        "stylelint": "16.10.0",
        "stylelint-config-standard": "36.0.1",
        "typescript": "5.8.2",
        "typescript-eslint": "8.15.0",
        "vite": "6.0.7",
        "vite-plugin-checker": "0.9.0",
        "vite-plugin-mock-dev-server": "1.8.3",
        "vite-plugin-svgr": "4.3.0"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    }
}
