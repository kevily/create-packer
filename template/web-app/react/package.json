{
    "name": "react-vite",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "prepare": "husky install",
        "dev": "vite",
        "build": "tsc --noEmit && vite build",
        "preview": "vite preview",
        "up:vite": "pnpm up vite @vitejs/* -L",
        "format": "prettier --write \"**/*.{ts,js,tsx,jsx,json,css,scss,less}\"",
        "lint": "tsc --noEmit && eslint **/*.{tsx,ts,jsx,js} && stylelint **/*.{css,scss,less}",
        "lint:fix": "eslint **/*.{tsx,ts,jsx,js} --fix && stylelint **/*.{css,scss,less} --fix",
        "cz": "cz",
        "push": "npm run commit && git push",
        "commit": "git add . && npm run cz"
    },
    "dependencies": {
        "axios": "1.3.6",
        "define-zustand": "2.0.0",
        "immer": "10.0.1",
        "lodash-es": "4.17.21",
        "qs": "6.11.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-router-dom": "6.14.0",
        "react-use": "17.4.0",
        "zustand": "4.4.1"
    },
    "devDependencies": {
        "@commitlint/cli": "17.6.1",
        "@commitlint/config-conventional": "17.6.1",
        "@commitlint/cz-commitlint": "17.5.0",
        "@types/lodash-es": "4.17.7",
        "@types/mockjs": "1.0.7",
        "@types/node": "18.16.0",
        "@types/qs": "6.9.7",
        "@types/react": "18.0.38",
        "@types/react-dom": "18.0.11",
        "@typescript-eslint/eslint-plugin": "5.59.0",
        "@typescript-eslint/parser": "5.59.0",
        "@vitejs/plugin-react": "4.0.0",
        "autoprefixer": "10.4.14",
        "commitizen": "4.3.0",
        "cssnano": "6.0.0",
        "eslint": "8.39.0",
        "eslint-import-resolver-typescript": "3.5.5",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-prettier": "5.0.0",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "husky": "8.0.3",
        "inquirer": "8",
        "mockjs": "1.1.0",
        "postcss": "8.4.23",
        "postcss-import": "15.1.0",
        "postcss-nesting": "11.2.2",
        "prettier": "3.0.0",
        "stylelint": "15.7.0",
        "stylelint-config-standard": "33.0.0",
        "tailwindcss": "3.3.1",
        "typescript": "5.0.4",
        "vite": "4.3.1",
        "vite-plugin-checker": "0.6.1",
        "vite-plugin-mock-dev-server": "1.2.1",
        "vite-plugin-svgr": "3.2.0"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    }
}
