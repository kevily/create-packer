{
    "name": "react-rsbuild",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "prepare": "husky",
        "dev": "rsbuild dev",
        "build": "rsbuild build",
        "build:analyse": "rsbuild build --env-mode analyse",
        "build:rsdoctor": "cross-env RSDOCTOR=true rsbuild build",
        "preview": "rsbuild preview",
        "up:rsbuild": "pnpm up @rsbuild/* -L",
        "format": "prettier --write \"**/*.{ts,js,tsx,jsx,json,css,scss,less}\"",
        "lint": "tsc --noEmit && eslint --no-error-on-unmatched-pattern && stylelint **/*.{css,scss,less,ts,tsx}",
        "lint:fix": "eslint --fix && stylelint **/*.{css,scss,less,ts,tsx} --fix",
        "cz": "cz",
        "push": "npm run commit && git push",
        "commit": "git add . && npm run cz"
    },
    "dependencies": {
        "@emotion/react": "11.14.0",
        "@emotion/styled": "11.14.1",
        "@tanstack/react-query": "5.51.15",
        "axios": "1.7.9",
        "define-zustand": "3.4.0",
        "es-toolkit": "1.43.0",
        "immer": "10.0.1",
        "qs": "6.11.2",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-router": "7.5.0",
        "react-use": "17.5.0",
        "type-fest": "4.33.0",
        "zustand": "5.0.3"
    },
    "devDependencies": {
        "@commitlint/cli": "17.6.1",
        "@commitlint/config-conventional": "17.6.1",
        "@commitlint/cz-commitlint": "17.5.0",
        "@eslint/js": "9.15.0",
        "@rsbuild/core": "1.6.15",
        "@rsbuild/plugin-eslint": "1.2.0",
        "@rsbuild/plugin-react": "1.4.2",
        "@rsbuild/plugin-svgr": "1.2.3",
        "@rsbuild/plugin-type-check": "1.3.2",
        "@rsdoctor/rspack-plugin": "1.4.0",
        "@swc/plugin-emotion": "11.1.0",
        "@types/node": "18.16.0",
        "@types/qs": "6.9.7",
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
        "autoprefixer": "10.4.14",
        "commitizen": "4.3.0",
        "cross-env": "7.0.3",
        "cssnano": "6.0.0",
        "eslint": "9.17.0",
        "eslint-import-resolver-typescript": "3.7.0",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-react": "7.37.2",
        "eslint-plugin-react-hooks": "5.1.0",
        "globals": "15.12.0",
        "husky": "9.1.6",
        "postcss": "8.4.35",
        "postcss-import": "16.0.1",
        "postcss-nesting": "12.0.3",
        "postcss-styled-syntax": "0.6.4",
        "prettier": "3.2.5",
        "stylelint": "16.10.0",
        "stylelint-config-standard": "36.0.1",
        "stylelint-webpack-plugin": "5.0.0",
        "typescript": "5.8.2"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    }
}
