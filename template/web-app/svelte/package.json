{
    "name": "svelte",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "prepare": "husky install",
        "dev": "vite",
        "build": "npm run check && npm run lint && vite build",
        "build:analyse": "npm run check && npm run lint && vite build --mode analyse",
        "preview": "vite preview",
        "up:vite": "pnpm up vite @vitejs/* -L",
        "up:svelte": "pnpm up svelte svelte-check @sveltejs/*  -L",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "pnpm run check && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "cz": "cz",
        "push": "npm run commit && git push",
        "commit": "git add . && npm run cz"
    },
    "devDependencies": {
        "@commitlint/cli": "17.7.1",
        "@commitlint/config-conventional": "17.7.0",
        "@commitlint/cz-commitlint": "17.7.1",
        "@rollup/plugin-eslint": "9.0.5",
        "@sveltejs/adapter-auto": "3.2.2",
        "@sveltejs/kit": "2.5.18",
        "@sveltejs/vite-plugin-svelte": "3.0.0",
        "@types/lodash-es": "4.17.9",
        "@typescript-eslint/eslint-plugin": "7.0.2",
        "@typescript-eslint/parser": "7.0.2",
        "commitizen": "4.3.0",
        "cssnano": "6.0.1",
        "eslint": "8.56.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-svelte": "2.35.1",
        "postcss": "8.4.35",
        "postcss-import": "16.0.1",
        "postcss-nesting": "12.0.3",
        "prettier": "3.2.5",
        "prettier-plugin-svelte": "3.2.5",
        "rollup-plugin-visualizer": "5.12.0",
        "svelte-check": "3.8.4",
        "tslib": "2.6.2",
        "typescript": "5.5.2",
        "vite": "5.3.3",
        "vite-plugin-stylelint": "5.3.1"
    },
    "dependencies": {
        "lodash-es": "4.17.21",
        "svelte": "4.2.18"
    }
}
