{
    "name": "react-app-webpack",
    "version": "1.0.0",
    "main": "./dist/index",
    "module": "./dist/index.esm.js",
    "private": true,
    "author": "1K",
    "scripts": {
        "prepare": "husky install",
        "dev": "webpack serve --env mode=dev",
        "build": "webpack --env mode=prod",
        "build:analyzer": "webpack --env mode=analyzer",
        "up:vite": "pnpm up vite @vitejs/* -L",
        "format": "prettier --write \"**/*.{ts,js,tsx,jsx,json,css,scss,less}\"",
        "lint": "tsc --noEmit && eslint **/*.{tsx,ts,jsx,js} --no-error-on-unmatched-pattern && stylelint **/*.{css,scss,less}",
        "lint:fix": "eslint **/*.{tsx,ts,jsx,js} --fix && stylelint **/*.{css,scss,less} --fix",
        "cz": "cz",
        "push": "npm run commit && git push",
        "commit": "git add . && npm run cz"
    },
    "dependencies": {
        "1k-types": "1.1.2",
        "axios": "1.5.0",
        "define-zustand": "2.0.1",
        "immer": "10.0.2",
        "lodash-es": "4.17.21",
        "qs": "6.11.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-router-dom": "6.14.0",
        "react-use": "17.4.0",
        "tailwindcss": "3.3.3",
        "zustand": "4.4.1"
    },
    "devDependencies": {
        "@commitlint/cli": "17.7.1",
        "@commitlint/config-conventional": "17.7.0",
        "@commitlint/cz-commitlint": "17.7.1",
        "@svgr/webpack": "8.1.0",
        "@types/lodash-es": "4.17.8",
        "@types/qs": "6.9.7",
        "@types/react": "18.2.21",
        "@types/react-dom": "18.2.7",
        "@types/react-redux": "7.1.26",
        "@types/react-router-config": "5.0.7",
        "@types/react-router-dom": "5.3.3",
        "@typescript-eslint/eslint-plugin": "6.17.0",
        "@typescript-eslint/parser": "6.17.0",
        "autoprefixer": "10.4.15",
        "commitizen": "4.3.0",
        "css-loader": "6.8.1",
        "cssnano": "6.0.1",
        "dotenv": "16.3.1",
        "esbuild-loader": "4.0.1",
        "eslint": "8.48.0",
        "eslint-import-resolver-typescript": "3.5.5",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-prettier": "5.0.0",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-webpack-plugin": "4.0.1",
        "fork-ts-checker-webpack-plugin": "8.0.0",
        "html-webpack-plugin": "5.5.3",
        "husky": "8.0.3",
        "mini-css-extract-plugin": "2.7.6",
        "postcss-import": "15.1.0",
        "postcss-loader": "7.3.3",
        "postcss-nesting": "12.0.1",
        "prettier": "3.0.2",
        "react-css-modules": "4.7.11",
        "react-test-renderer": "18.2.0",
        "rimraf": "5.0.1",
        "style-loader": "3.3.3",
        "stylelint": "15.10.3",
        "stylelint-config-standard": "34.0.0",
        "stylelint-webpack-plugin": "4.1.1",
        "typescript": "5.3.3",
        "webpack": "5.88.2",
        "webpack-bundle-analyzer": "4.9.1",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "4.15.1",
        "webpackbar": "5.0.2"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    }
}
