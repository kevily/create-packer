{
    "name": "react-app-webpack",
    "version": "1.0.0",
    "main": "./dist/index",
    "module": "./dist/index.esm.js",
    "private": true,
    "author": "1K",
    "scripts": {
        "prepare": "husky install",
        "dev": "webpack serve --env mode=dev",
        "build": "webpack --env mode=prod",
        "build:analyzer": "webpack --env mode=analyzer",
        "up:webpack": "pnpm up webpackbar webpack webpack-* -L",
        "format": "prettier --write \"**/*.{ts,js,tsx,jsx,json,css,scss,less}\"",
        "lint": "tsc --noEmit && eslint --no-error-on-unmatched-pattern && stylelint **/*.{css,scss,less,ts,tsx}",
        "lint:fix": "eslint --fix && stylelint **/*.{css,scss,less,ts,tsx} --fix",
        "cz": "cz",
        "push": "npm run commit && git push",
        "commit": "git add . && npm run cz"
    },
    "dependencies": {
        "@tanstack/react-query": "5.51.15",
        "axios": "1.7.9",
        "define-zustand": "3.1.1",
        "immer": "10.0.3",
        "lodash-es": "4.17.21",
        "qs": "6.11.2",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-router-dom": "6.14.0",
        "react-use": "17.5.0",
        "styled-components": "6.1.11",
        "zustand": "4.4.1"
    },
    "devDependencies": {
        "@commitlint/cli": "18.6.1",
        "@commitlint/config-conventional": "18.6.2",
        "@commitlint/cz-commitlint": "18.6.1",
        "@svgr/webpack": "8.1.0",
        "@types/lodash-es": "4.17.12",
        "@types/qs": "6.9.11",
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
        "autoprefixer": "10.4.17",
        "commitizen": "4.3.0",
        "css-loader": "6.10.0",
        "cssnano": "6.0.3",
        "dotenv": "16.4.5",
        "esbuild-loader": "4.2.2",
        "eslint": "9.17.0",
        "eslint-import-resolver-typescript": "3.7.0",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-react": "7.37.2",
        "eslint-plugin-react-hooks": "5.1.0",
        "eslint-webpack-plugin": "4.2.0",
        "fork-ts-checker-webpack-plugin": "9.0.2",
        "globals": "15.12.0",
        "html-webpack-plugin": "5.6.0",
        "husky": "9.1.6",
        "mini-css-extract-plugin": "2.8.0",
        "postcss-import": "16.0.1",
        "postcss-loader": "8.1.0",
        "postcss-nesting": "12.0.3",
        "postcss-styled-syntax": "0.6.4",
        "prettier": "3.2.5",
        "rimraf": "5.0.5",
        "style-loader": "3.3.4",
        "stylelint": "16.10.0",
        "stylelint-config-standard": "36.0.1",
        "stylelint-webpack-plugin": "5.0.0",
        "typescript": "5.7.2",
        "typescript-eslint": "8.15.0",
        "webpack": "5.97.1",
        "webpack-bundle-analyzer": "4.10.2",
        "webpack-cli": "6.0.1",
        "webpack-dev-server": "5.2.0",
        "webpackbar": "7.0.0"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    }
}
