{
    "name": "vue-vite",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "prepare": "husky install",
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "preview": "vite preview",
        "format": "prettier --write \"src/**/*.{ts,js,tsx,jsx,vue,css,scss,less}\" \"./package.json\"",
        "lint": "eslint ./src/**/*.{ts,vue} && stylelint ./src/**/*.{css,scss,less}",
        "lint:fix": "eslint ./src/**/*.{ts,vue} --fix && stylelint ./src/**/*.{css,scss,less} --fix",
        "cz": "cz",
        "push": "npm run commit && git push",
        "commit": "git add . && npm run cz"
    },
    "dependencies": {
        "axios": "1.3.6",
        "lodash-es": "4.17.21",
        "pinia": "2.0.35",
        "vue": "3.3.4",
        "vue-router": "4.2.1"
    },
    "devDependencies": {
        "@commitlint/cli": "17.6.1",
        "@commitlint/config-conventional": "17.6.1",
        "@commitlint/cz-commitlint": "17.5.0",
        "@types/lodash-es": "4.17.7",
        "@types/mockjs": "1.0.7",
        "@typescript-eslint/eslint-plugin": "5.59.0",
        "@typescript-eslint/parser": "5.59.0",
        "@vitejs/plugin-vue": "4.2.3",
        "@vitejs/plugin-vue-jsx": "3.0.1",
        "autoprefixer": "10.4.14",
        "commitizen": "4.3.0",
        "cssnano": "6.0.0",
        "eslint": "8.39.0",
        "eslint-import-resolver-typescript": "3.5.5",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-prettier": "5.0.0",
        "eslint-plugin-vue": "9.13.0",
        "husky": "8.0.3",
        "inquirer": "8",
        "mockjs": "1.1.0",
        "postcss": "8.4.23",
        "postcss-import": "15.1.0",
        "postcss-nesting": "11.2.2",
        "prettier": "3.0.0",
        "stylelint": "15.7.0",
        "stylelint-config-standard": "33.0.0",
        "tailwindcss": "3.3.1",
        "typescript": "5.0.4",
        "vite": "4.3.8",
        "vite-plugin-checker": "0.6.1",
        "vite-plugin-mock-dev-server": "1.2.1",
        "vite-svg-loader": "4.0.0",
        "vue-tsc": "1.6.5"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    }
}
