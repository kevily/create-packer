{
    "name": "vue-vite",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "prepare": "husky install",
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "build:analyse": "tsc --noEmit && vite build --mode analyse",
        "preview": "vite preview",
        "format": "prettier --write \"**/*.{ts,js,tsx,jsx,json,vue,css,scss,less}\"",
        "lint": "vue-tsc --noEmit && eslint **/*.{ts,tsx,js,jsx,vue} --no-error-on-unmatched-pattern && stylelint **/*.{css,scss,less}",
        "lint:fix": "eslint **/*.{ts,tsx,js,jsx,vue} --fix && stylelint **/*.{css,scss,less} --fix",
        "cz": "cz",
        "push": "npm run commit && git push",
        "commit": "git add . && npm run cz",
        "up:vite": "pnpm up vite @vitejs/* vite-plugin-* -L"
    },
    "dependencies": {
        "1k-types": "1.2.0",
        "axios": "1.3.6",
        "lodash-es": "4.17.21",
        "pinia": "2.1.7",
        "vue": "3.4.21",
        "vue-router": "4.3.0"
    },
    "devDependencies": {
        "@commitlint/cli": "17.6.1",
        "@commitlint/config-conventional": "17.6.1",
        "@commitlint/cz-commitlint": "17.5.0",
        "@nabla/vite-plugin-eslint": "2.0.2",
        "@types/lodash-es": "4.17.12",
        "@types/mockjs": "1.0.10",
        "@typescript-eslint/eslint-plugin": "7.6.0",
        "@typescript-eslint/parser": "7.6.0",
        "@vitejs/plugin-vue": "4.6.2",
        "@vitejs/plugin-vue-jsx": "3.1.0",
        "autoprefixer": "10.4.14",
        "commitizen": "4.3.0",
        "cssnano": "6.0.0",
        "eslint": "8.56.0",
        "eslint-import-resolver-typescript": "3.6.1",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-prettier": "5.1.3",
        "eslint-plugin-vue": "9.21.1",
        "husky": "8.0.3",
        "inquirer": "^8.1.2",
        "mockjs": "1.1.0",
        "postcss": "8.4.38",
        "postcss-import": "16.1.0",
        "postcss-nesting": "12.1.1",
        "prettier": "3.2.5",
        "rollup-plugin-visualizer": "5.12.0",
        "stylelint": "16.3.1",
        "stylelint-config-standard-scss": "13.1.0",
        "tailwindcss": "3.4.3",
        "typescript": "5.3.3",
        "vite": "4.5.2",
        "vite-plugin-mock-dev-server": "1.5.0",
        "vite-plugin-stylelint": "5.3.1",
        "vite-svg-loader": "5.1.0",
        "vue-tsc": "2.0.11"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    }
}
