{
    "name": "vue-vite",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "prepare": "husky install",
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "build:analyse": "tsc --noEmit && vite build --mode analyse",
        "preview": "vite preview",
        "format": "prettier --write \"**/*.{vue,ts,js,tsx,jsx,json,css,scss,less}\"",
        "lint": "vue-tsc --noEmit && eslint --no-error-on-unmatched-pattern && stylelint **/*.{css,scss,less,vue}",
        "lint:fix": "eslint --fix && stylelint **/*.{css,scss,less,vue} --fix",
        "cz": "cz",
        "push": "npm run commit && git push",
        "commit": "git add . && npm run cz",
        "up:vite": "pnpm up vite @vitejs/* vite-plugin-* -L",
        "up:vue": "pnpm up vue vue-router vue-tsc -L"
    },
    "dependencies": {
        "axios": "1.7.9",
        "es-toolkit": "1.39.8",
        "pinia": "2.3.1",
        "tailwindcss": "4.1.4",
        "type-fest": "4.33.0",
        "vue": "3.5.13",
        "vue-router": "4.5.0"
    },
    "devDependencies": {
        "@commitlint/cli": "17.6.1",
        "@commitlint/config-conventional": "17.6.1",
        "@commitlint/cz-commitlint": "17.5.0",
        "@eslint/js": "9.15.0",
        "@faker-js/faker": "8.4.1",
        "@tailwindcss/vite": "4.1.4",
        "@types/mockjs": "1.0.10",
        "@vitejs/plugin-vue": "5.2.1",
        "@vitejs/plugin-vue-jsx": "4.1.1",
        "autoprefixer": "10.4.14",
        "commitizen": "4.3.0",
        "cssnano": "6.0.0",
        "eslint": "9.17.0",
        "eslint-import-resolver-typescript": "3.7.0",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-vue": "9.32.0",
        "globals": "15.12.0",
        "husky": "9.1.6",
        "inquirer": "8.1.2",
        "postcss": "8.4.38",
        "postcss-html": "1.8.0",
        "postcss-import": "16.1.0",
        "postcss-nesting": "12.1.1",
        "prettier": "3.2.5",
        "rollup-plugin-visualizer": "5.12.0",
        "stylelint": "16.10.0",
        "stylelint-config-standard-scss": "13.1.0",
        "typescript": "5.8.2",
        "typescript-eslint": "8.15.0",
        "vite": "6.0.7",
        "vite-plugin-checker": "0.9.0",
        "vite-plugin-mock-dev-server": "1.8.3",
        "vite-svg-loader": "5.1.0",
        "vue-tsc": "2.2.8"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    }
}
