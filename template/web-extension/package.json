{
    "name": "web-extension",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "prepare": "husky install",
        "dev": "wxt",
        "dev:firefox": "wxt -b firefox",
        "build": "wxt build",
        "build_dev": "wxt build --mode dev",
        "build:firefox": "wxt build -b firefox",
        "build_dev:firefox": "wxt build -b firefox --mode dev",
        "zip": "wxt zip",
        "zip_dev": "wxt zip --mode dev",
        "zip:firefox": "wxt zip -b firefox",
        "zip_dev:firefox": "wxt zip -b firefox --mode dev",
        "postinstall": "wxt prepare",
        "up:vite": "pnpm up vite @vitejs/* -L",
        "format": "prettier --write \"**/*.{ts,js,tsx,jsx,json,css,scss,less}\"",
        "lint": "tsc --noEmit && eslint **/*.{tsx,ts} && stylelint **/*.{css,scss,less,ts,tsx}",
        "lint:fix": "eslint **/*.{tsx,ts} --fix && stylelint **/*.{css,scss,less,ts,tsx} --fix",
        "cz": "cz",
        "push": "npm run commit && git push",
        "commit": "git add . && npm run cz"
    },
    "dependencies": {
        "@tanstack/react-query": "5.51.15",
        "@vanilla-extract/css": "1.17.4",
        "@webext-core/messaging": "2.2.0",
        "axios": "1.7.9",
        "define-zustand": "3.4.0",
        "es-toolkit": "1.39.8",
        "immer": "10.0.1",
        "qs": "6.11.2",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "type-fest": "4.33.0",
        "zustand": "5.0.3"
    },
    "devDependencies": {
        "@commitlint/cli": "17.6.1",
        "@commitlint/config-conventional": "17.6.1",
        "@commitlint/cz-commitlint": "17.5.0",
        "@eslint/js": "9.15.0",
        "@types/chrome": "0.0.254",
        "@types/node": "18.16.0",
        "@types/qs": "6.9.7",
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
        "@vanilla-extract/vite-plugin": "5.1.1",
        "@vitejs/plugin-react": "5.0.1",
        "autoprefixer": "10.4.14",
        "commitizen": "4.3.0",
        "cssnano": "6.0.0",
        "eslint": "9.17.0",
        "eslint-import-resolver-typescript": "3.7.0",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-react": "7.37.2",
        "eslint-plugin-react-hooks": "5.1.0",
        "globals": "15.12.0",
        "husky": "9.1.6",
        "inquirer": "^8.1.2",
        "postcss": "8.4.31",
        "postcss-import": "15.1.0",
        "postcss-nesting": "11.2.2",
        "postcss-scss": "4.0.9",
        "postcss-styled-syntax": "0.6.4",
        "prettier": "3.2.5",
        "rimraf": "5.0.1",
        "sass": "1.63.4",
        "stylelint": "16.10.0",
        "stylelint-config-standard": "36.0.1",
        "typescript": "5.8.2",
        "typescript-eslint": "8.28.0",
        "vite": "7.1.3",
        "vite-plugin-svgr": "4.5.0",
        "wxt": "0.20.5"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    }
}
