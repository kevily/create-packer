{
    "name": "web-extension",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "prepare": "husky install",
        "dev": "vite",
        "build": "tsc --noEmit && vite build",
        "preview": "vite preview",
        "up:vite": "pnpm up vite @vitejs/* -L",
        "format": "prettier --write \"**/*.{ts,js,tsx,jsx,json,css,scss,less,json}\"",
        "lint": "tsc --noEmit && eslint **/*.{tsx,ts} && stylelint **/*.{css,scss,less}",
        "lint:fix": "eslint **/*.{tsx,ts} --fix && stylelint **/*.{css,scss,less} --fix",
        "cz": "cz",
        "push": "npm run commit && git push",
        "commit": "git add . && npm run cz"
    },
    "dependencies": {
        "define-zustand": "3.1.0",
        "immer": "10.0.1",
        "ky": "1.1.3",
        "lodash-es": "4.17.21",
        "qs": "6.11.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "zustand": "4.4.1"
    },
    "devDependencies": {
        "1k-types": "1.1.2",
        "@commitlint/cli": "17.6.1",
        "@commitlint/config-conventional": "17.6.1",
        "@commitlint/cz-commitlint": "17.5.0",
        "@crxjs/vite-plugin": "2.0.0-beta.18",
        "@nabla/vite-plugin-eslint": "2.0.2",
        "@types/chrome": "0.0.254",
        "@types/lodash-es": "4.17.7",
        "@types/node": "18.16.0",
        "@types/qs": "6.9.7",
        "@types/react": "18.0.38",
        "@types/react-dom": "18.0.11",
        "@typescript-eslint/eslint-plugin": "7.0.2",
        "@typescript-eslint/parser": "7.0.2",
        "@vitejs/plugin-react": "4.2.1",
        "autoprefixer": "10.4.14",
        "commitizen": "4.3.0",
        "cssnano": "6.0.0",
        "eslint": "8.56.0",
        "eslint-import-resolver-typescript": "3.6.1",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-prettier": "5.1.3",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "husky": "8.0.3",
        "inquirer": "8",
        "postcss": "8.4.31",
        "postcss-import": "15.1.0",
        "postcss-nesting": "11.2.2",
        "postcss-scss": "4.0.9",
        "prettier": "3.2.5",
        "rimraf": "5.0.1",
        "sass": "1.63.4",
        "stylelint": "16.2.1",
        "stylelint-config-standard-scss": "13.0.0",
        "tailwindcss": "3.3.7",
        "typescript": "5.3.3",
        "vite": "4.5.2",
        "vite-plugin-stylelint": "5.3.1",
        "vite-plugin-svgr": "4.2.0"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    }
}
