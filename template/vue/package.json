{
    "name": "vue-vite",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "prepare": "husky install",
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "preview": "vite preview",
        "format": "prettier --write \"src/**/*.{ts,js,tsx,jsx,vue,css,scss,less}\" \"./package.json\"",
        "lint": "eslint ./src/**/*.{ts,vue} && stylelint ./src/**/*.{css,scss,less}",
        "lint:fix": "eslint ./src/**/*.{ts,vue} --fix && stylelint ./src/**/*.{css,scss,less} --fix",
        "cz": "cz",
        "push": "npm run commit && git push",
        "commit": "git add . && npm run cz"
    },
    "dependencies": {
        "lodash-es": "4.17.21",
        "pinia": "2.0.30",
        "vite-plugin-eslint": "1.8.1",
        "vue": "3.2.47",
        "vue-router": "4.1.6"
    },
    "devDependencies": {
        "@commitlint/cli": "17.4.2",
        "@commitlint/config-conventional": "^17.4.2",
        "@commitlint/cz-commitlint": "17.4.2",
        "@types/lodash-es": "4.17.6",
        "@typescript-eslint/eslint-plugin": "5.51.0",
        "@typescript-eslint/parser": "5.51.0",
        "@vitejs/plugin-vue": "4.0.0",
        "autoprefixer": "10.4.13",
        "commitizen": "4.3.0",
        "cssnano": "5.1.14",
        "eslint": "8.34.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-vue": "9.9.0",
        "husky": "8.0.3",
        "inquirer": "8",
        "postcss": "8.4.21",
        "postcss-import": "15.1.0",
        "postcss-nesting": "11.1.0",
        "prettier": "2.8.4",
        "stylelint": "15.0.0",
        "stylelint-config-standard": "30.0.1",
        "tailwindcss": "3.2.6",
        "typescript": "4.9.5",
        "vite": "4.1.1",
        "vue-tsc": "1.0.24"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    }
}
