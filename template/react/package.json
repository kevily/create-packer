{
    "name": "react-vite",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "prepare": "husky install",
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "up:vite": "pnpm up vite @vitejs/* -L",
        "format": "prettier --write \"src/**/*.{ts,js,tsx,jsx,css,scss,less}\" \"./package.json\"",
        "lint": "eslint ./src/**/*.{tsx,ts} && stylelint ./src/**/*.{css,scss,less}",
        "lint:fix": "eslint ./src/**/*.{tsx,ts} --fix && stylelint ./src/**/*.{css,scss,less} --fix",
        "test:watchs": "jest --watch",
        "test": "jest",
        "test:coverage": "jest --silent --watchAll=false --coverage",
        "cz": "cz",
        "push": "npm run commit && git push",
        "commit": "git add . && npm run cz"
    },
    "dependencies": {
        "immer": "9.0.16",
        "lodash-es": "4.17.21",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-router-dom": "6.4.3",
        "zustand": "4.1.4"
    },
    "devDependencies": {
        "@testing-library/dom": "8.19.0",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "13.4.0",
        "@testing-library/user-event": "14.4.3",
        "@types/jest": "29.2.3",
        "@types/lodash-es": "4.17.6",
        "@types/react": "18.0.25",
        "@types/react-dom": "18.0.9",
        "@types/testing-library__jest-dom": "5.14.5",
        "@typescript-eslint/eslint-plugin": "5.44.0",
        "@typescript-eslint/parser": "5.44.0",
        "@vitejs/plugin-react": "2.2.0",
        "autoprefixer": "10.4.13",
        "commitizen": "4.2.5",
        "cssnano": "5.1.14",
        "cz-adapter-eslint": "0.3.0",
        "eslint": "8.28.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.31.11",
        "eslint-plugin-react-hooks": "4.6.0",
        "husky": "8.0.2",
        "jest": "29.3.1",
        "jest-environment-jsdom": "29.3.1",
        "postcss": "8.4.19",
        "postcss-import": "15.0.0",
        "postcss-nesting": "10.2.0",
        "prettier": "2.7.1",
        "stylelint": "14.15.0",
        "stylelint-config-standard": "29.0.0",
        "tailwindcss": "3.2.4",
        "typescript": "4.9.3",
        "vite": "3.2.4",
        "vite-plugin-eslint": "1.8.1"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-adapter-eslint"
        }
    }
}
