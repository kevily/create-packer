{
    "compilerOptions": {
        "baseUrl": ".",
        "strict": true,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "strictPropertyInitialization": false,
        "esModuleInterop": true,
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "types": ["vite/client"],
        "noEmit": true,
        "isolatedModules": true,
        "paths": {
            "@/*": ["src/*"]
        }
    }
}
